{
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "Launch protractor",
                "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
                "args": ["${workspaceRoot}/protractorConfig.js"]
            },
            {
                "type": "node",
                "request": "launch",
                "name": "Launch Program",
                "program": "${file}",
                "outFiles": [
                    "${workspaceRoot}/out/**/*.js"
                ]
            }
        ]
    }